<resources>

    <!-- GT APP Style -->

    <!--<style name="GtIndex_RelativeLayout">-->
        <!--<item name="android:background">@drawable/gt_button_bg</item>-->
        <!--<item name="android:layout_width">161dp</item>-->
        <!--<item name="android:layout_height">match_parent</item>-->
